<div id="<%= dom_id client %>">
  <tr>
    <th scope="row"><%= client.id %></th>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movie %></td>
    <td><%= link_to "Editar Cliente", edit_client_path(client), class: "btn btn-primary" %></td>
    <td><%= link_to "Eliminar Cliente", @client, method: :delete, class: "btn btn-danger" %></td>
  </tr>
</div>
